{
  "expo": {
    "name": "Logistica",
    "slug": "Logistica_De_Entrega",
    "version": "2.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f2f2f2"
  },
  "updates": {
    "fallbackToCacheTimeout": 0
  },
  "assetBundlePatterns": [
    "**/*"
  ],
    "android": {
      "package": "com.wallaceramos.Logistica",
      "versionCode": 17,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#f2f2f2"
      
      },
      "permissions": [
        "INTERNET",
        "READ_INTERNAL_STORAGE"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.wallaceramos.Logistica",
      "buildNumber": "3.1.3",
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
    
  }
}
